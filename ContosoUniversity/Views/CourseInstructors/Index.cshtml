@model IEnumerable<ContosoUniversity.DTOs.CourseInstructorDTO>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Instructor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Course)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Instructor.ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Course.CourseID)
                </td>               
            </tr>
        }
    </tbody>
</table>
